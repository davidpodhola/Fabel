{
    "env": "node",
    "projFile": "./fable-sample-react.fsproj",
    "babelPlugins": ["transform-runtime"],
    "targets": {
        "debug": {
            "watch": true,
            "sourceMaps": true,
            "symbols": ["WEBPACK_DEV_SERVER"],
            "scripts": {
                "postbuild": "node server"
            }
        },
        "production": {
            "sourceMaps": false,
            "symbols": [],
            "scripts": {
                "postbuild": "node node_modules/webpack/bin/webpack -p"
            }
        }
    }
}